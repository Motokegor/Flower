ight: 0;\r\n      border: 1px solid #43ffd2;\r\n      left: 0px;\r\n      top: 60px;\r\n    }\r\n  }\r\n}\r\n.accordion { \r\n  padding-bottom: 20vh;\r\n  &__item {\r\n    border: 1px solid #43ffd2;\r\n    padding: 20px;\r\n    width: 80%;\r\n    .question {\r\n      display: flex;\r\n      justify-content: space-between;\r\n      align-items: center;\r\n    }\r\n    .question-title {\r\n      font-weight: 400;\r\n      font-size: 20px;\r\n      line-height: 30px;\r\n      text-transform: uppercase;\r\n      color: #43ffd2;\r\n    }\r\n    .question-plus {\r\n      width: 50px;\r\n      height: 50px;\r\n      display: flex;\r\n      justify-content: center;\r\n      align-items: center;\r\n      &:hover {\r\n        background: #0d4437;\r\n        border-radius: 50%;\r\n        cursor: pointer;\r\n      }\r\n      .question-span {\r\n        background: #43ffd2;\r\n        width: 14px;\r\n        height: 2px;\r\n      }\r\n      .question-span2 {\r\n        transform: rotate(90deg);\r\n        margin-left: -14px;\r\n        background: #43ffd2;\r\n        width: 14px;\r\n        height: 2px;\r\n      }\r\n    }\r\n    .answer {\r\n      display: none;\r\n      .answer-text {\r\n        font-weight: 300;\r\n        font-size: 20px;\r\n        line-height: 30px;\r\n        color: #ffffff;\r\n      }\r\n    }\r\n  }\r\n  .accordion__item-active {\r\n    .spanMinus1 {\r\n      display: none;\r\n    }\r\n    .spanMinus2 {\r\n      display: none;\r\n    }\r\n    .spanMinus3 {\r\n      display: none;\r\n    }\r\n    .spanMinus4 {\r\n      display: none;\r\n    }\r\n    .spanMinus5 {\r\n      display: none;\r\n    }\r\n    .spanMinus6 {\r\n      display: none;\r\n    }\r\n    .spanMinus7 {\r\n      display: none;\r\n    }\r\n    .answer {\r\n      display: block;\r\n    }\r\n  }\r\n}\r\n\r\n@media (max-width: 800px) {\r\n  .accordion {\r\n    .accordion__item {\r\n      width: 95%;\r\n    }\r\n  }\r\n  .main {\r\n    padding-left: 20px;\r\n\r\n    .faq-title {\r\n      h2 {